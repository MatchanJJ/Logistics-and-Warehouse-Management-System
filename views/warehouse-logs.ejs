<link rel="stylesheet" href="/styles.css"> <!-- Link to your CSS file -->

<body>
    <a href="/warehouses">
        <img class="back" src="/back-button.png" alt="back">
    </a>
    <h1>Warehouse Logs</h1>
    <table class="table table-dark table-striped">
        <thead>
            <tr>
                <th>Log ID</th>
                <th>Timestamp</th>
                <th>Warehouse ID</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            <% logs.forEach(log => { %>
                <tr>
                    <td><%= log.warehouse_log_id %></td>
                    <td><%= log.date_time %></td>
                    <td><%= log.warehouse_id %></td>
                    <td><%= log.warehouse_log_description %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
