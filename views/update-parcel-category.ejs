<link rel="stylesheet" href="/styles.css">
<body>
    <div class="container">
        <h2>Update Parcel Category</h2>
        <form action="/update-parcel-category/<%= parcelCategory.parcel_category_id %>" method="POST">
            <label for="parcel_category_name">Parcel Category Name:</label>
            <input type="text" id="parcel_category_name" name="parcel_category_name" value="<%= parcelCategory.parcel_category_name %>" required>
            <button type="submit">Update Parcel Category</button>
        </form>
        
    </div>
    
</body>
