
<body>

    <a href="/manage-parcel-categories" class="btn ">
        <img class="back" src="/back-button.png" alt="back" class="back-button"> 
    </a>

    <div class="form-container ">
        <h2 class="text-white">Update Parcel Category</h2>

        <form action="/update-parcel-category/<%= parcelCategory.parcel_category_id %>" method="POST" class="mt-3 p-4 border rounded ">
            <div class="form-group">
                <label for="parcel_category_name">Parcel Category Name:</label>
                <input type="text" id="parcel_category_name" name="parcel_category_name" value="<%= parcelCategory.parcel_category_name %>" class="form-control" required>
            </div>

            <div class="form-group">
                <button type="submit" class="btn btn-primary">Update Parcel Category</button>
            </div>
        </form>
        
    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
