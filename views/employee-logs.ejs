
<body>
    <a href="/employees">
        <img class="back" src="/back-button.png" alt="back">
    </a>
    <h1>Employee Logs</h1>
    <table>
        <thead>
            <tr>
                <th>Log ID</th>
                <th>Timestamp</th>
                <th>Employee ID</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            <% employeeLogs.forEach(log => { %>
                <tr>
                    <td><%= log.employee_log_id %></td>
                    <td><%= log.date_time %></td>
                    <td><%= log.employee_id %></td>
                    <td><%= log.employee_log_description %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
