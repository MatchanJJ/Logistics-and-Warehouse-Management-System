
<body>
    <a href="/product">
        <img class="back" src="/back-button.png" alt="back">
    </a> 
        <h1>Product Inventory Logs</h1>

    <main>
        <% if (inventoryLogs && inventoryLogs.length > 0) { %>
            <table>
                <thead>
                    <tr>
                        <th>Log ID</th>
                        <th>Timestamp</th>
                        <th>Product ID</th>
                        <th>Warehouse ID</th>
                        <th>Description</th>
                    </tr>
                </thead>
                <tbody>
                    <% inventoryLogs.forEach(log => { %>
                        <tr>
                            <td><%= log.product_inventory_log_id %></td>
                            <td><%= log.date_time %></td>
                            <td><%= log.date_time %></td>
                            <td><%= log.warehouse_id %></td>
                            <td><%= log.product_inventory_log_description %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        <% } else { %>
            <p>No product inventory logs found.</p>
        <% } %>
    </main>

</body>
