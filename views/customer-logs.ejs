
<body>
    <a href="/customers">
        <img class="back" src="/back-button.png" alt="back">
    </a>
    <h1>Customer Logs</h1>
    <table class="table table-dark table-striped">
        <thead>
            <tr>
                <th>Log ID</th>
                <th>Timestamp</th>
                <th>Customer ID</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            <% customerLogs.forEach(log => { %>
                <tr>
                    <td><%= log.customer_log_id %></td>
                    <td><%= log.date_time %></td>
                    <td><%= log.customer_id %></td>
                    <td><%= log.customer_log_description %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
