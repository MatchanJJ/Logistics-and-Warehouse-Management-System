<a href="/customers">
    <img class="back" src="/back-button.png" alt="back">
</a>
<body>
    <h1>Customer Archives</h1>
    <table class="table table-dark table-striped">
        <thead>
            <tr>
                <th>Archive ID</th>
                <th>Customer ID</th>
                <th>Customer First Name</th>
                <th>Customer Last Name</th>
                <th>Customer Email</th>
                <th>Customer Address</th>
            </tr>
        </thead>
        <tbody>
            <% customerArchives.forEach(archive => { %>
                <tr>
                    <td><%= archive.archive_id %></td>
                    <td><%= archive.customer_id %></td>
                    <td><%= archive.customer_first_name %></td>
                    <td><%= archive.customer_last_name %></td>
                    <td><%= archive.customer_email %></td>
                    <td><%= archive.customer_address %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
