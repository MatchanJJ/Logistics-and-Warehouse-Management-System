
        <body>
            <a href="/">
                <img class="home" src="/home-button.png" alt="home">
            </a>
        </a>
        <a href="/order-logs">
            <img class="home" src="/log.png" alt="log">
        </a>
        <a href="/order-archives">
            <img class="home" src="/inbox.png" alt="archive">
        </a>
                <h2>Orders</h2>
                <link rel="stylesheet" href="/styles.css">
                    <a href="/add-orders" class="btn">Add Order</a>
                    <a href="/manage-orders" class="btn">Order Management</a>
                    <a href="/manage-status-and-category" class="btn">Status and Category Management</a>
            
            <table>
                <thead>
                    <tr>
                        <th>Order ID</th>
                        <th>Customer ID</th>
                        <th>Order Timestamp</th>
                        <th>Order Status ID</th>
                        <th>Shipping Service ID</th>
                        <th>Delivery Address</th>
                        <th>Shipping Receiver</th>
                        <th>Order Type ID</th>
                        <th>Order Total Amount</th>
                        <th>Parcel ID</th>
                        <th>Parcel Total Price</th>
                        <th>Product ID</th>
                        <th>Product Quantity</th>
                        <th>Product Total Price</th>
                        <th>Shipping Service Name</th>
                    </tr>
                </thead>
                <tbody>
                    <% orders.forEach(order => { %>
                        <tr>
                            <td><%= order.order_id %></td>
                            <td><%= order.customer_id %></td>
                            <td><%= order.order_date_time %></td>
                            <td><%= order.order_status_id %></td>
                            <td><%= order.shipping_service_id %></td>
                            <td><%= order.delivery_address %></td>
                            <td><%= order.shipping_receiver %></td>
                            <td><%= order.order_type_id %></td>
                            <td><%= order.order_total_amount %></td>
                            <td><%= order.parcel_id %></td>
                            <td><%= order.postal_total_price %></td>
                            <td><%= order.product_id %></td>
                            <td><%= order.product_quantity %></td>
                            <td><%= order.product_total_price %></td>
                            <td><%= order.shipping_service_name %></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </body>