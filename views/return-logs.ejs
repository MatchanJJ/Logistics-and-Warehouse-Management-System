<body>
    <a href="/returns">
        <img class="back" src="/back-button.png" alt="back">
    </a>
    <h1>Return Logs</h1>
    <table>
        <thead>
            <tr>
                <th>Log ID</th>
                <th>Timestamp</th>
                <th>Return ID</th>
                <th>Log Message</th>
            </tr>
        </thead>
        <tbody>
            <% returnLogs.forEach(log => { %>
                <tr>
                    <td><%= log.return_log_id %></td>
                    <td><%= log.date_time %></td>
                    <td><%= log.return_id %></td>
                    <td><%= log.return_log_description %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
