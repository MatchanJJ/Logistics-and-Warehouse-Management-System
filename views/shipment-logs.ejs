<link rel="stylesheet" href="/styles.css">
<body>
    <a href="/shipments">
        <img class="back" src="/back-button.png" alt="hbackome">
    </a>
    <h1>Shipment Logs</h1>
    <table class="table table-dark table-striped">
        <thead>
            <tr>
                <th>Log ID</th>
                <th>Timestamp</th>
                <th>Shipment ID</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            <% shipmentLogs.forEach(log => { %>
                <tr>
                    <td><%= log.shipment_log_id %></td>
                    <td><%= log.date_time %></td>
                    <td><%= log.shipment_id %></td>
                    <td><%= log.shipment_log_description %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
