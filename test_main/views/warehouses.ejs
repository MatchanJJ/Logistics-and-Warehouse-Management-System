<h2>Warehouse List</h2>
<div>
    <a href="/add-warehouse" class="btn">Add Warehouse</a>
    <a href="/manage-partner" class="btn">Manage Logistics Partner</a>
    <a href="/check-availability" class="btn">Check Warehouse Availabiity</a>

    <link rel="stylesheet" href="/styles.css"> <!-- Link to your CSS file -->
</div>
<% if (warehouses && warehouses.length > 0) { %>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Location</th>
                <th>Capacity</th>
                <th>Warehouse Type ID</th>
            </tr>
        </thead>
        <tbody>
            <% warehouses.forEach(warehouse => { %>
            <tr>
                <td><%= warehouse.warehouse_id %></td>
                <td><%= warehouse.warehouse_address %></td>
                <td><%= warehouse.capacity %></td>
                <td><%= warehouse.warehouse_type_id %></td>
                <td>
                    <a href="/update-warehouse/<%= warehouse.warehouse_id %>" class="btn">Update</a> 
                </td>
            </tr>
            <% }) %>
        </tbody>
    </table>
<% } else { %>
    <p>No warehouses found.</p>
<% } %>
